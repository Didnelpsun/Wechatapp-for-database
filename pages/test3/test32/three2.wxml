          </movable-view>
          <movable-view class="move" direction="all" bindchange="t6" hidden="{{s6}}">
            <text>6</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t5" hidden="{{s5}}">
            <text>5</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t4" hidden="{{s4}}">
            <text>4</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t3" hidden="{{s3}}">
            <text>3</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t2" hidden="{{s2}}">
