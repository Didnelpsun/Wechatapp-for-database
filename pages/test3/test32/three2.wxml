          </movable-view>
          <movable-view class="obj" x="{{clientWidth/3*2}}" y="{{clientHeight/4}}">码
          </movable-view>
          <movable-view class="obj" x="{{clientWidth/5}}" y="{{clientHeight/2.5}}">域
          </movable-view>
          <movable-view class="obj" x="{{clientWidth/5*2}}" y="{{clientHeight/2.5}}">分量
          </movable-view>
          <movable-view class="obj" x="{{clientWidth/5*3}}" y="{{clientHeight/2.5}}">关系模式
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t12" hidden="{{s12}}">
            <text>12</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t11" hidden="{{s11}}">
            <text>11</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t10" hidden="{{s10}}">
            <text>10</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t9" hidden="{{s9}}">
            <text>9</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t8" hidden="{{s8}}">
            <text>8</text>
          </movable-view>
          <movable-view class="move" direction="all" bindchange="t7" hidden="{{s7}}">
